{
  "author": "igorlogius",
  "homepage_url": "https://github.com/igorlogius/close-duplicate-tabs",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_area": "navbar"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Alt+C"
      }
    },
    "ignore-set": {
      "description": "Ignore selected tabs"
    },
    "ignore-clear": {
      "description": "Clear ignored tabs"
    }
  },
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "options_ui": {
    "page": "options.html"
  },
  "name": "Close Duplicate Tabs",
  "description": "Shows count of duplicate tabs and close them quickly",
  "permissions": ["storage", "cookies", "tabs"],
  "version": "1.0.17"
}
